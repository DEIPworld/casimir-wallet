{
    "family": "frontend-task",
    "executionRoleArn": "arn:aws:iam::777345813910:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "frontend-task",
            "image": "",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "fe_wallet_logs",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "streaming"
                }
            },
            "environment": [],
            "memory": 2048,
            "cpu": 1024
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "1024",
    "memory": "2048"
}