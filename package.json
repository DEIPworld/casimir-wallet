{
  "name": "deip-wallet",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:testnet": "npm run build -- --mode development",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix --ignore-path .gitignore",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "linkModules": "node ./scripts/linkDevModules.js"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0",
    "@deip/chain-service": "1.381.0",
    "@deip/commands": "^1.380.0",
    "@deip/toolbox": "^1.380.0",
    "@mdi/font": "^6.6.96",
    "@mdi/js": "^6.6.96",
    "@polkadot/api": "^8.7.1",
    "@polkadot/ui-keyring": "^2.4.1",
    "@sentry/tracing": "^7.0.0",
    "@sentry/vue": "^7.0.0",
    "bignumber.js": "^9.0.2",
    "cryptext": "^1.1.4",
    "date-fns": "^2.28.0",
    "pinia": "^2.0.14",
    "pinia-plugin-persistedstate-2": "^1.0.0",
    "qrcode": "^1.5.0",
    "string-crypto": "^2.0.2",
    "tiny-emitter": "^2.1.0",
    "vee-validate": "^4.5.11",
    "vue": "^3.2.33",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.0.15",
    "vuetify": "^3.0.0-beta.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@deip/eslint-config": "^1.375.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.33",
    "@types/rollup-plugin-node-builtins": "^2.1.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vue/tsconfig": "^0.1.3",
    "@vuetify/vite-plugin": "^1.0.0-alpha.10",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.1",
    "npm-check-updates": "^12.0.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-polyfill-node": "^0.9.0",
    "sass": "^1.51.0",
    "symlink-dir": "^5.0.1",
    "typescript": "~4.6.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.4",
    "vue-tsc": "^0.34.13"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "eslint --fix"
  },
  "engines": {
    "node": "^14.15.0",
    "npm": "^6.0.0"
  }
}
