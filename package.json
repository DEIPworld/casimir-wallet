    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-vue": "^9.2.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "inquirer": "^9.0.0",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "^15.2.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-polyfill-node": "^0.10.1",
    "sass": "^1.53.0",
    "symlink-dir": "^5.0.1",
    "typescript": "~4.7.4",
    "vite": "^2.9.14",
    "vitest": "^0.17.1",
