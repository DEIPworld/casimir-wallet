{
  "name": "deip-wallet",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:testnet": "npm run build -- --mode development",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix --ignore-path .gitignore",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "linkModules": "node ./scripts/linkDevModules.js"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0",
    "@deip/chain-service": "1.389.0",
    "@deip/commands": "^1.389.0",
    "@deip/messages": "^1.389.0",
    "@deip/toolbox": "^1.388.0",
    "@mdi/font": "^6.9.96",
    "@mdi/js": "^6.9.96",
    "@polkadot/api": "^8.11.3",
    "@polkadot/ui-keyring": "^2.7.2",
    "@sentry/tracing": "^7.5.1",
    "@sentry/vue": "^7.5.1",
    "@types/qs": "^6.9.7",
    "axios": "^0.27.2",
    "bignumber.js": "^9.0.2",
    "cryptext": "^1.1.4",
    "date-fns": "^2.28.0",
    "pinia": "^2.0.14",
    "pinia-plugin-persistedstate-2": "^1.0.0",
    "qrcode": "^1.5.0",
    "qs": "^6.11.0",
    "string-crypto": "^2.0.2",
    "tiny-emitter": "^2.1.0",
    "vee-validate": "^4.5.11",
    "vue": "^3.2.37",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.1.1",
    "vuetify": "^3.0.0-beta.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@deip/eslint-config": "^1.375.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^18.0.3",
    "@types/rollup-plugin-node-builtins": "^2.1.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-typescript": "^11.0.0",
